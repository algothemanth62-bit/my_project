#include<bits/stdc++.h>
using namespace std;
void insertion(int a[],int n)
{
   for(int i=1;i<n;i++)
   {
    int j=i;
    while(j>0&&a[j-1]>a[j])
    {
      int t=a[j];
      a[j]=a[j-1];
      a[j-1]=t;
      j--;
    }
  }
   
 }
int main()
{
int n;
cout<<"enter the size:";
cin>>n;
int a[n];
cout<<"enter the values:";
for(int i=0;i<n;i++)
{
  cin>>a[i];
}
insertion(a,n);
for(int c:a)
  cout<<c<<" ";
}
 
 
